
<table>                         
  <tr>                          
    <th>Title</th>
    <th>Firstname</th>
    <th>Surname</th>
    <th>Date Of Birth</th>
    <th>Telephone No</th>
    <th>Contact Type</th>
    <th>Email</th>
  </tr>
<% @property.tenants.each do |tenant| %>      
  <tr>
    <td><%= tenant.title %></td>  
    <td><%= tenant.firstname %></td> 
    <td><%= tenant.surname %></td>
    <td><%= tenant.dateofbirth %></td>
    <td><%= tenant.telno %></td>
    <td><%= tenant.contact_type %></td>
  </tr>
<% end %>               
</table>    

 <table>                         
              <tr>                          
              <th>Council Name</th>
              <th>Council Email </th>
            </tr>
          <% @property.councils.each do |council| %>      
            <tr>
              <td><%= council.name %></td>   
               <td><%= council.email %></td>  
            </tr>
          <% end %>               
      </table> 

