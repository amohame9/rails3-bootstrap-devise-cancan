<h1>tenants Form</h1>



<%= simple_form_for @property, :url => url_for(:action => 'update', :controller =>   'properties/build'), :method => 'put' do |f| %>
  <%= f.simple_fields_for :tenants do |builder| %>
      <%= builder.input :title %>
	  <%= builder.input :firstname %>
	  <%= builder.input :surname %>
	  <%= builder.input :dateofbirth %>
	  <%= builder.input :telno %>
	  <%= builder.input :contact_type %>
	  <%= builder.input :email %>
	  <%= builder.submit %>
  <% end %>
<% end %>

<%= link_to 'skip', next_wizard_path %>